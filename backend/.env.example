# Environment variables for EventGo Backend
# Copy this file to .env and fill in your actual values

# Node Environment
NODE_ENV=production

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRY=7d

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here

# DynamoDB Tables
DYNAMODB_USERS_TABLE=Datta-Evntgo-Datta-2005-users
DYNAMODB_EVENTS_TABLE=Datta-Evntgo-Datta-2005-events
DYNAMODB_OPPORTUNITIES_TABLE=Datta-Evntgo-Datta-2005-opportunities

# S3 Configuration
S3_BUCKET_NAME=eventgo-media-bucket
S3_REGION=us-east-1
CLOUDFRONT_DOMAIN_NAME=your_cloudfront_domain_here

# Server Configuration
SERVER_PORT=3000
FRONTEND_URL=http://localhost
SESSION_SECRET=your-session-secret-change-in-production
